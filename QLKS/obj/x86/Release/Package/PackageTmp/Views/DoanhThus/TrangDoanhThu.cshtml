@model IEnumerable<QLKS.ChiTietDoanhThu>


<table style="border:1px solid black;margin-top:30px;" class="table">
    <tr style="border:1px solid black;">
        <th style="border:1px solid black;">
            @Html.DisplayNameFor(model => model.LoaiPhong)
        </th>
        <th style="border:1px solid black;">
            @Html.DisplayNameFor(model => model.DoanhThu)
        </th>
        <th style="border:1px solid black;">
            @Html.DisplayNameFor(model => model.Thang)
        </th>
        <th style="border:1px solid black;">
            @Html.DisplayNameFor(model => model.Tile)
        </th>
        <th style="border:1px solid black;">
            Chức năng
        </th>
    </tr>

@foreach (var item in Model) {
    <tr style="border:1px solid black;">
        <td style="border:1px solid black;">
            @Html.DisplayFor(modelItem => item.LoaiPhong)
        </td>
        <td style="border:1px solid black;">
            @Html.DisplayFor(modelItem => item.DoanhThu)
        </td>
        <td style="border:1px solid black;">
            @Html.DisplayFor(modelItem => item.Thang)
        </td>
        <td style="border:1px solid black;">
            @Html.DisplayFor(modelItem => item.Tile)
        </td>
        <td style="border:1px solid black;">
            @Html.ActionLink("Xóa", "DeleteChiTietDoanhThu", new { id=item.MaDoanhThu }, new { @class = "btn btn-primary1" })
        </td>
    </tr>
}

</table>
